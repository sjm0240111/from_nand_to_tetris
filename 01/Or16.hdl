/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Not16(in=a, out=Nota);
    Not16(in=b, out=Notb);
    And16(a=Nota, b=Notb, out=NotaAndNotb);
    Not16(in=NotaAndNotb, out=out);
}